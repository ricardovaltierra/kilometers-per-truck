<% provide :page, "New City" %>
<%= link_to "<-", :back %>

<h1><%= current_user.name %></h1>

<h1>Register new City</h1>

<%= form_with model: @group, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :icon %>
  <%= f.text_field :icon %>

  <%= f.submit :submit %>
<% end %>